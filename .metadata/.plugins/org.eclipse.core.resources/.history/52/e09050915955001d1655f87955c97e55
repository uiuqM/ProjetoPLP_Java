		this.view = view;
	}
	
	public void updateTable() {
		//pega lista com agendamento;
		AgendamentoDAO agendamentoDAO = new AgendamentoDAO();
		ArrayList<Agendamento> = agendamentoDAO.selectAll();
		//exibir lista na view;
		
	}
}
